   <property name="styleSheet">
    <string notr="true">#centralwidget{
	background-color: #fefeff;
}

*{
	margin: 0;
	padding: 0;
	color: #000;
	border: none;
}
#leftMenu, QCustomSlideMenu{
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0 rgb(0, 85, 255), stop:1 rgb(0, 170, 255));
}

#leftMenu{
	color: white;
}

QLineEdit{
	background: transparent;
}

#searchFrame{
	border-radius: 10px;
	border: 2px solid rgb(0, 85, 255);
}

#appHeader{
	color: rgb(0, 170, 255);
}

#card1, #card2, #card3, #card4{
	background-color: #fefeff;
	border-radius:  15px;
}

#seeMoreButton{
	background-color: #f6f2ff;
	color: #2596be;
	padding: 7px;
	border-radius: 10px;
	box-shadow: 0px 0px 10px 0px #000;
}

#irinaServiceLabel{
	color: #fefeff;
}

#menuFrame QPushButton{
	text-align: left;
	padding: 5px 15px;
}

#menuFrame #accueilBtn{
	background-color: #fefeff;
	border-top-left-radius: 15px;
}

#profileContainer{
	background-color: #fefeff;
	border-radius: 20px;
}

#logoutBtn{
	padding: 4px 7px;
	border: 1px solid #000;
	border-radius: 7px
}


#welcomeLabel, #stock_labele, #facture_title, #notif_label, #journal_label, #reliure_label{
	text-align: center;
	color:  rgb(0, 170, 255);
	background-color: #e5e0ff;
	padding: 10px 15px;
}

#total_payer{
	color: #f56614;
}

#valider_commandeBtn{
	background: #d5d2ff;
	padding: 7px;
	border-radius: 7px;
}

#stockTable{
	background-color: rgb(244, 244, 255);
}

#filter_labele{
	padding: 5px;
}

#filterCombo, #dateEdit, #dateEdit_2{
	padding: 5px;
	border-radius: 10px;
	background-color: rgb(255, 220, 219);
}

#avance_field{
	border: 1px solid rgb(0, 85, 255);
	padding-left: 5px;
	border-radius: 5px;
}

#selection_rapide_combo, #quantite_spinBox{
	padding-left: 5px;
	border: 1px solid rgb(0, 85, 255);
	border-radius: 5px;
}

#add_selection_rapideBtn, #delete_type, #modify_reliure{
	border: 2px solid rgb(0, 170, 255);
	border-radius: 7px;
	padding: 2px;
	cursor: pointer;
}

#reset_reliure{
	border: 2px solid orange;
	border-radius: 7px;
	padding: 2px;
	cursor: pointer;
}

#submit_section_rapideBtn, #add_type, #submit_reliure{
	border: 2px solid rgb(0, 255, 127);
	border-radius: 7px;
	padding: 2px;
	cursor: pointer;
}

#reliure_form{
	border: 2px solid rgb(0, 170, 255);
	border-radius: 15px;
}</string>
